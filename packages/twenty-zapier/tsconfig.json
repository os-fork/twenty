{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "outDir": "lib",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "src/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules", "lib"]
}
